import telebot
from telebot import types

# –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∏—Ä—É–π—Ç–µ –±–æ—Ç–∞ —Å –≤–∞—à–∏–º —Ç–æ–∫–µ–Ω–æ–º
bot = telebot.TeleBot("6425671606:AAEL9iDyfK_DlJbfTEltuHtA9tefXI0prxI")

# –°–æ–∑–¥–∞–π—Ç–µ –∫–ª–∞–≤–∏–∞—Ç—É—Ä—É —Å –∫–Ω–æ–ø–∫–∞–º–∏
keyboard = types.ReplyKeyboardMarkup(resize_keyboard=True)

# –°–æ–∑–¥–∞–π—Ç–µ –∫–Ω–æ–ø–∫–∏ –∏ –¥–æ–±–∞–≤—å—Ç–µ –∏—Ö –≤ –∫–ª–∞–≤–∏–∞—Ç—É—Ä—É
button1 = types.KeyboardButton("–ö–Ω–æ–ø–∫–∞ 1")
button2 = types.KeyboardButton("–ö–Ω–æ–ø–∫–∞ 2")
button3 = types.KeyboardButton("–ö–Ω–æ–ø–∫–∞ 3")

# –î–æ–±–∞–≤—å—Ç–µ –∫–Ω–æ–ø–∫–∏ –∫ –∫–ª–∞–≤–∏–∞—Ç—É—Ä–µ
keyboard.add(button1, button2)
keyboard.add(button3)

# –û—Ç–ø—Ä–∞–≤—å—Ç–µ —Å–æ–æ–±—â–µ–Ω–∏–µ —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä–æ–π –≤ —á–∞—Ç
@bot.message_handler(commands=['start'])
def send_welcome(message):
    bot.reply_to(message, "–ü—Ä–∏–≤–µ—Ç! –Ø –±–æ—Ç —Å –∫–Ω–æ–ø–∫–∞–º–∏. –í—ã–±–µ—Ä–∏ –æ–ø—Ü–∏—é:", reply_markup=keyboard)

# –û–±—Ä–∞–±–æ—Ç–∫–∞ –Ω–∞–∂–∞—Ç–∏–π –Ω–∞ –∫–Ω–æ–ø–∫–∏
@bot.message_handler(func=lambda message: True)
def handle_buttons(message):
    if message.text == "–ö–Ω–æ–ø–∫–∞ 1":
        bot.reply_to(message, "–í—ã –Ω–∞–∂–∞–ª–∏ –Ω–∞ –ö–Ω–æ–ø–∫—É 1")
    elif message.text == "–ö–Ω–æ–ø–∫–∞ 2":
        bot.reply_to(message, "–í—ã –Ω–∞–∂–∞–ª–∏ –Ω–∞ –ö–Ω–æ–ø–∫—É 2")
    elif message.text == "–ö–Ω–æ–ø–∫–∞ 3":
        bot.reply_to(message, "–í—ã –Ω–∞–∂–∞–ª–∏ –Ω–∞ –ö–Ω–æ–ø–∫—É 3")

# –ó–∞–ø—É—Å–∫ –±–æ—Ç–∞


import telebot
from telebot import types

# –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∏—Ä—É–π—Ç–µ –≤–∞—à–µ–≥–æ –±–æ—Ç–∞
bot = telebot.TeleBot("6425671606:AAEL9iDyfK_DlJbfTEltuHtA9tefXI0prxI")

# –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –∫–æ–º–∞–Ω–¥—ã /start
@bot.message_handler(commands=['start'])
def start(message):
    user_id = message.chat.id

    # –°–æ–∑–¥–∞–π—Ç–µ –∫–ª–∞–≤–∏–∞—Ç—É—Ä—É
    keyboard = types.ReplyKeyboardMarkup(resize_keyboard=True)
    request_contact_button = types.KeyboardButton("–ü—Ä–µ–¥–æ—Å—Ç–∞–≤–∏—Ç—å –Ω–æ–º–µ—Ä —Ç–µ–ª–µ—Ñ–æ–Ω–∞", request_contact=True)
    keyboard.add(request_contact_button)

    # –û—Ç–ø—Ä–∞–≤—å—Ç–µ —Å–æ–æ–±—â–µ–Ω–∏–µ —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä–æ–π
    bot.send_message(user_id, "–ù–∞–∂–º–∏—Ç–µ –∫–Ω–æ–ø–∫—É '–ü—Ä–µ–¥–æ—Å—Ç–∞–≤–∏—Ç—å –Ω–æ–º–µ—Ä —Ç–µ–ª–µ—Ñ–æ–Ω–∞', —á—Ç–æ–±—ã –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–∏—Ç—å —Å–≤–æ–π –Ω–æ–º–µ—Ä —Ç–µ–ª–µ—Ñ–æ–Ω–∞:", reply_markup=keyboard)

# –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª–µ–Ω–Ω–æ–≥–æ –∫–æ–Ω—Ç–∞–∫—Ç–∞
@bot.message_handler(content_types=['contact'])
def handle_contact(message):
    user_id = message.chat.id
    contact = message.contact

    # –ò–∑–≤–ª–µ–∫–∏—Ç–µ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ –∫–æ–Ω—Ç–∞–∫—Ç–µ
    phone_number = contact.phone_number
    first_name = contact.first_name

    response = f"–í—ã –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–∏–ª–∏ —Å–≤–æ–π –Ω–æ–º–µ—Ä —Ç–µ–ª–µ—Ñ–æ–Ω–∞: {phone_number}\n–ò–º—è: {first_name}"
    bot.send_message(user_id, response)

# –ó–∞–ø—É—Å—Ç–∏—Ç–µ –≤–∞—à–µ–≥–æ –±–æ—Ç–∞
bot.polling()

