pip install pyTelegramBotAPI profanity-check

import telebot
from profanity_check import predict

# –ó–∞–º–µ–Ω–∏—Ç–µ 'YOUR_API_TOKEN' –Ω–∞ –≤–∞—à —Ç–æ–∫–µ–Ω API –±–æ—Ç–∞
bot = telebot.TeleBot('YOUR_API_TOKEN')

@bot.message_handler(func=lambda message: True)
def check_for_profanity(message):
    # –ê–Ω–∞–ª–∏–∑–∏—Ä—É–µ–º —Ç–µ–∫—Å—Ç —Å–æ–æ–±—â–µ–Ω–∏—è –Ω–∞ –Ω–∞–ª–∏—á–∏–µ –º–∞—Ç–µ—Ä–Ω—ã—Ö —Å–ª–æ–≤
    text = message.text
    is_profanity = any(predict([text]))
    
    if is_profanity:
        # –ï—Å–ª–∏ —Å–æ–æ–±—â–µ–Ω–∏–µ —Å–æ–¥–µ—Ä–∂–∏—Ç –º–∞—Ç, —É–¥–∞–ª—è–µ–º –µ–≥–æ
        bot.delete_message(message.chat.id, message.message_id)

# –ó–∞–ø—É—Å–∫–∞–µ–º –±–æ—Ç–∞
bot.polling(none_stop=True)


//// –ø—Ä–æ–≤–µ—Ä–∫–∞ json

    @bot.message_handler(commands=['bot'])
    def handle_start(message):
        bot.send_message(message.chat.id, '–ü—Ä–∏–≤–µ—Ç! –û—Ç–ø—Ä–∞–≤—å—Ç–µ –º–Ω–µ –¥–∞–Ω–Ω—ã–µ, –∏ —è —Å–æ—Ö—Ä–∞–Ω—é –∏—Ö –≤ JSON.')

    @bot.message_handler(func=lambda message: True)
    def handle_message(message):
        user_id = message.from_user.id
        user_data = {
            'user_id': user_id,
            'message_text': message.text
        }

        with open(f'users/{user_id}.json', 'w') as file:
            json.dump(user_data, file)
        bot.send_message(message.chat.id, '–î–∞–Ω–Ω—ã–µ —Å–æ—Ö—Ä–∞–Ω–µ–Ω—ã –≤ JSON.')


        /// –ò–Ω—Ç–µ—Ä–µ—Å–Ω–æ–µ

@bot.message_handler(commands=['start'])
def start(message):
    keyboard_markup = types.ReplyKeyboardMarkup(row_width=2)
    btn_today = types.KeyboardButton('/today')
    btn_tomorrow = types.KeyboardButton('tomorrow')
    keyboard_markup.add(btn_today, btn_tomorrow)
    bot.send_message(message.chat.id, 'clear_schedule', reply_markup=keyboard_markup)


@bot.message_handler(commands=["today"])
def add_user_handler(message):
    bot.send_message(message.chat.id, '–Ω—É –≤–æ—Ç —Ç–µ–±–µ –∏ today')

