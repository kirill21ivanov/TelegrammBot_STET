    @bot.message_handler(commands=['a'])
    def message(message):
        if message.chat.id == config.ADMIN or message.chat.id == config.ADMIN2:
            for user in file_users:
                admin = types.InlineKeyboardMarkup(row_width=1)
                ok = types.InlineKeyboardButton("–•–æ—Ä–æ—à–æ!", callback_data="block")
                admin.add(ok)
                try:
                    user_id = message.chat.id
                    bot.delete_message(user_id, message.message_id)
                except telebot.apihelper.ApiTelegramException as e:
                    print(f"–û—à–∏–±–∫–∞ –ø—Ä–∏ —É–¥–∞–ª–µ–Ω–∏–∏ —Å–æ–æ–±—â–µ–Ω–∏—è: {e}")
                trimmed_text = message.text[message.text.find(' '):].strip()
                if (trimmed_text == 'a'):
                    print("error")
                else:
                    try:
                        bot.send_message(user, f"üîî–†–∞—Å—Å—ã–ª–∫–∞:\n{trimmed_text}", reply_markup=admin)
                    except telebot.apihelper.ApiTelegramException as e:
                        print(f"–û—à–∏–±–∫–∞ –ø—Ä–∏ —É–¥–∞–ª–µ–Ω–∏–∏ —Å–æ–æ–±—â–µ–Ω–∏—è: {e}")
        else:
            bot.send_message(message.chat.id, "–£ –≤–∞—Å –Ω–µ—Ç –ø—Ä–∞–≤ –≤—ã–ø–æ–ª–Ω–∏—Ç—å –¥–∞–Ω–Ω—É—é –∫–æ–º–∞–Ω–¥—É.")
            return 0
        


        ////
–í–º–µ—Å—Ç–æ `message.text` –≤ —ç—Ç–æ–º –∫–æ–¥–µ –º–æ–∂–Ω–æ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å –¥—Ä—É–≥–∏–µ –∞—Ç—Ä–∏–±—É—Ç—ã –æ–±—ä–µ–∫—Ç–∞ `message` –¥–ª—è –±–æ–ª–µ–µ —Ç–æ—á–Ω–æ–≥–æ –æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏—è —Å–æ–æ–±—â–µ–Ω–∏—è, –∫–æ—Ç–æ—Ä–æ–µ –≤—ã —Ö–æ—Ç–∏—Ç–µ —É–¥–∞–ª–∏—Ç—å. –í –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç –≤–∞—à–µ–π –∫–æ–Ω–∫—Ä–µ—Ç–Ω–æ–π –∑–∞–¥–∞—á–∏ –∏ —Å—Ç—Ä—É–∫—Ç—É—Ä—ã –≤–∞—à–µ–≥–æ –∫–æ–¥–∞, –≤—ã –º–æ–∂–µ—Ç–µ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å —Å–ª–µ–¥—É—é—â–∏–µ –∞—Ç—Ä–∏–±—É—Ç—ã `message`:

1. `message.message_id`: –≠—Ç–æ—Ç –∞—Ç—Ä–∏–±—É—Ç —Å–æ–¥–µ—Ä–∂–∏—Ç —É–Ω–∏–∫–∞–ª—å–Ω—ã–π –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä —Å–æ–æ–±—â–µ–Ω–∏—è. –í—ã –º–æ–∂–µ—Ç–µ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å –µ–≥–æ, —á—Ç–æ–±—ã —É–∫–∞–∑–∞—Ç—å –∫–æ–Ω–∫—Ä–µ—Ç–Ω–æ–µ —Å–æ–æ–±—â–µ–Ω–∏–µ –¥–ª—è —É–¥–∞–ª–µ–Ω–∏—è.

```python
bot.delete_message(message.chat.id, message.message_id)
```

2. `message.reply_to_message`: –ï—Å–ª–∏ –≤—ã —Ö–æ—Ç–∏—Ç–µ —É–¥–∞–ª–∏—Ç—å –æ—Ç–≤–µ—Ç –Ω–∞ –æ–ø—Ä–µ–¥–µ–ª–µ–Ω–Ω–æ–µ —Å–æ–æ–±—â–µ–Ω–∏–µ, –≤—ã –º–æ–∂–µ—Ç–µ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å —ç—Ç–æ—Ç –∞—Ç—Ä–∏–±—É—Ç –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ —Å–æ–æ–±—â–µ–Ω–∏–∏, –Ω–∞ –∫–æ—Ç–æ—Ä–æ–µ –±—ã–ª –¥–∞–Ω –æ—Ç–≤–µ—Ç, –∏ –∑–∞—Ç–µ–º —É–¥–∞–ª–∏—Ç—å —ç—Ç–æ —Å–æ–æ–±—â–µ–Ω–∏–µ.

```python
if message.reply_to_message:
    bot.delete_message(message.chat.id, message.reply_to_message.message_id)
```

3. `message.from_user.id`: –ï—Å–ª–∏ –≤–∞–º –Ω—É–∂–Ω–æ —É–¥–∞–ª–∏—Ç—å —Å–æ–æ–±—â–µ–Ω–∏—è —Ç–æ–ª—å–∫–æ –æ—Ç –æ–ø—Ä–µ–¥–µ–ª–µ–Ω–Ω–æ–≥–æ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è, –≤—ã –º–æ–∂–µ—Ç–µ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å –∞—Ç—Ä–∏–±—É—Ç `from_user` –¥–ª—è –¥–æ—Å—Ç—É–ø–∞ –∫ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ –∏ –µ–≥–æ `id`.

```python
if message.from_user.id == desired_user_id:
    bot.delete_message(message.chat.id, message.message_id)
```

–ó–∞–º–µ–Ω–∏—Ç–µ `desired_user_id` –Ω–∞ –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è, —á—å–∏ —Å–æ–æ–±—â–µ–Ω–∏—è –≤—ã —Ö–æ—Ç–∏—Ç–µ —É–¥–∞–ª–∏—Ç—å.

–í—ã–±–æ—Ä –ø–æ–¥—Ö–æ–¥—è—â–µ–≥–æ –∞—Ç—Ä–∏–±—É—Ç–∞ –∑–∞–≤–∏—Å–∏—Ç –æ—Ç –≤–∞—à–µ–π –∫–æ–Ω–∫—Ä–µ—Ç–Ω–æ–π –ª–æ–≥–∏–∫–∏ —É–¥–∞–ª–µ–Ω–∏—è —Å–æ–æ–±—â–µ–Ω–∏–π –≤ –≤–∞—à–µ–º –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–∏ Telegram.


/////


import telebot

# –ó–∞–º–µ–Ω–∏—Ç–µ 'YOUR_API_TOKEN' –Ω–∞ –≤–∞—à —Ç–æ–∫–µ–Ω API –±–æ—Ç–∞
bot = telebot.TeleBot('YOUR_API_TOKEN')

@bot.message_handler(commands=['start'])
def handle_start(message):
    # –û—Ç–ø—Ä–∞–≤–ª—è–µ–º —Ç–µ–∫—Å—Ç –∏ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ
    text = "–ü—Ä–∏–≤–µ—Ç! –≠—Ç–æ —Ç–µ–∫—Å—Ç –∫ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—é."
    with open('path/to/your/image.jpg', 'rb') as photo:
        bot.send_photo(message.chat.id, photo, caption=text, parse_mode="HTML")

# –ó–∞–ø—É—Å–∫–∞–µ–º –±–æ—Ç–∞
bot.polling(none_stop=True)



