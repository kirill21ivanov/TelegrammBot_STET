// –ó–ê–ü–ò–°–¨ –í –§–ê–ô–õ –° –ù–£–ú–ï–†–ê–¶–ò–ï–ô
@bot.message_handler(func=lambda message: True)
def handle_user_input(message):
    try:
        # –û—Ç–∫—Ä–æ–π—Ç–µ —Ñ–∞–π–ª –¥–ª—è —á—Ç–µ–Ω–∏—è –∏ –ø–æ–ª—É—á–∏—Ç–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Å—Ç—Ä–æ–∫
        with open("bk.txt", 'r') as fail:
            lin = fail.readlines()
            number_of_lines = len(lin)
        
        # –¢–µ–ø–µ—Ä—å –æ—Ç–∫—Ä–æ–π—Ç–µ —Ñ–∞–π–ª –¥–ª—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –∏ –¥–æ–±–∞–≤—å—Ç–µ –Ω–æ–≤—É—é —Å—Ç—Ä–æ–∫—É
        with open("bk.txt", 'a') as fail:
            fail.write(f"{number_of_lines + 1}: User ID: {message.from_user.id}, Message: {message.text}\n")
    except Exception as error:
        print(f"–ü—Ä–æ–∏–∑–æ—à–ª–∞ –æ—à–∏–±–∫–∞: {error}")


//–í–´–í–û–î–ò–¢ –í–°–ï –ß–¢–û –ï–°–¢–¨ –í –§–ê–ô–õ–ï
    with open("bk.txt", "r") as file:
        file_contents = file.read()
        print(file_contents)
        bot.send_message(message.chat.id, file_contents)

//–ë–ê–ù
import telebot

# –í–∞—à —Ç–æ–∫–µ–Ω –±–æ—Ç–∞
TOKEN = '–≤–∞—à_—Ç–æ–∫–µ–Ω'

# –°–æ–∑–¥–∞–µ–º –æ–±—ä–µ–∫—Ç –±–æ—Ç–∞
bot = telebot.TeleBot(TOKEN)

# –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤—ã–¥–∞—á–∏ –º—É—Ç–∞ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—é –ø–æ –µ–≥–æ ID
def mute_user(user_id, duration_seconds):
    try:
        # –í—ã–∑—ã–≤–∞–µ–º –º–µ—Ç–æ–¥ restrict_chat_member –¥–ª—è –≤—ã–¥–∞—á–∏ –º—É—Ç–∞
        bot.restrict_chat_member(chat_id=chat_id, user_id=user_id, can_send_messages=False, until_date=duration_seconds)
        return True  # –£—Å–ø–µ—à–Ω–æ –≤—ã–¥–∞–Ω –º—É—Ç
    except telebot.apihelper.ApiTelegramException as e:
        print(f"–û—à–∏–±–∫–∞ –ø—Ä–∏ –≤—ã–¥–∞—á–µ –º—É—Ç–∞: {e}")
        return False  # –ü—Ä–æ–∏–∑–æ—à–ª–∞ –æ—à–∏–±–∫–∞ –ø—Ä–∏ –≤—ã–¥–∞—á–µ –º—É—Ç–∞

# –ü—Ä–∏–º–µ—Ä –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è —Ñ—É–Ω–∫—Ü–∏–∏ –¥–ª—è –≤—ã–¥–∞—á–∏ –º—É—Ç–∞ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—é
user_id_to_mute = 123456789  # –ó–∞–º–µ–Ω–∏—Ç–µ –Ω–∞ ID –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è, –∫–æ—Ç–æ—Ä–æ–º—É —Ö–æ—Ç–∏—Ç–µ –≤—ã–¥–∞—Ç—å –º—É—Ç
duration_seconds = 3600  # –ü—Ä–æ–¥–æ–ª–∂–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å –º—É—Ç–∞ –≤ —Å–µ–∫—É–Ω–¥–∞—Ö (1 —á–∞—Å –≤ –¥–∞–Ω–Ω–æ–º —Å–ª—É—á–∞–µ)

if mute_user(user_id_to_mute, duration_seconds):
    print(f"–ú—É—Ç –≤—ã–¥–∞–Ω –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—é —Å ID {user_id_to_mute} –Ω–∞ {duration_seconds} —Å–µ–∫—É–Ω–¥.")
else:
    print(f"–ù–µ —É–¥–∞–ª–æ—Å—å –≤—ã–¥–∞—Ç—å –º—É—Ç –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—é —Å ID {user_id_to_mute}.")

# –ó–∞–ø—É—Å–∫ –±–æ—Ç–∞
bot.polling()



@bot.message_handler(commands=['basa'])
def message(message):
    body = types.InlineKeyboardMarkup(row_width=1)
    button2 = types.InlineKeyboardButton("–°–∫—Ä—ã—Ç—å", callback_data="block")
    body.add(button2)
    if str(message.chat.id) != "870163214":
        bot.send_message(message.chat.id, "–£ –≤–∞—Å –Ω–µ—Ç –ø—Ä–∞–≤ –≤—ã–ø–æ–ª–Ω–∏—Ç—å –¥–∞–Ω–Ω—É—é –∫–æ–º–∞–Ω–¥—É.", reply_markup=body)
        return 0
    else:
        with open("bk.txt", 'r') as fail:
            lin = fail.readlines()
            number_of_lines = len(lin)
        if (number_of_lines == 0):
            try:
                user_id = message.chat.id
                bot.delete_message(user_id, message.message_id)
            except telebot.apihelper.ApiTelegramException as e:
                print(f"–û—à–∏–±–∫–∞ –ø—Ä–∏ —É–¥–∞–ª–µ–Ω–∏–∏ —Å–æ–æ–±—â–µ–Ω–∏—è: {e}")
            bot.send_message(message.chat.id, "–ë–∞–∑–∞ –¥–∞–Ω–Ω—ã—Ö –ø—É—Å—Ç–∞—è!", reply_markup=body)
        else:
            with open("bk.txt", "r") as file:
                body = types.InlineKeyboardMarkup(row_width=1)
                button2 = types.InlineKeyboardButton("–°–∫—Ä—ã—Ç—å", callback_data="block")
                body.add(button2)
                try:
                    user_id = message.chat.id
                    bot.delete_message(user_id, message.message_id)
                except telebot.apihelper.ApiTelegramException as e:
                    print(f"–û—à–∏–±–∫–∞ –ø—Ä–∏ —É–¥–∞–ª–µ–Ω–∏–∏ —Å–æ–æ–±—â–µ–Ω–∏—è: {e}")
                file_contents = file.read()
                print(file_contents)
                bot.send_message(message.chat.id, file_contents, reply_markup=body)


/// —É–¥–∞–ª–∏—Ç—å –≤—Å–µ –∏–∑ —Ñ–∞–π–ª–∞!!
    with open("bk.txt", "w") as file:
        pass


// –ø–µ—Ä–µ–Ω–æ—Å –¥–∞–Ω–Ω—ã—Ö
# –£–∫–∞–∂–∏—Ç–µ –ø—É—Ç–∏ –∫ –∏—Å—Ö–æ–¥–Ω–æ–º—É –∏ —Ü–µ–ª–µ–≤–æ–º—É —Ñ–∞–π–ª–∞–º
source_file_path = "–∏—Å—Ö–æ–¥–Ω—ã–π_—Ñ–∞–π–ª.txt"
target_file_path = "—Ü–µ–ª–µ–≤–æ–π_—Ñ–∞–π–ª.txt"

# –û—Ç–∫—Ä—ã–≤–∞–µ–º –∏—Å—Ö–æ–¥–Ω—ã–π —Ñ–∞–π–ª –¥–ª—è —á—Ç–µ–Ω–∏—è –∏ —Ü–µ–ª–µ–≤–æ–π —Ñ–∞–π–ª –¥–ª—è –∑–∞–ø–∏—Å–∏
with open(source_file_path, "r") as source_file, open(target_file_path, "w") as target_file:
    # –ß–∏—Ç–∞–µ–º —Å–æ–¥–µ—Ä–∂–∏–º–æ–µ –∏—Å—Ö–æ–¥–Ω–æ–≥–æ —Ñ–∞–π–ª–∞
    content = source_file.read()
    
    # –ó–∞–ø–∏—Å—ã–≤–∞–µ–º —Å–æ–¥–µ—Ä–∂–∏–º–æ–µ –≤ —Ü–µ–ª–µ–≤–æ–π —Ñ–∞–π–ª
    target_file.write(content)

print(f"–°–æ–¥–µ—Ä–∂–∏–º–æ–µ —Ñ–∞–π–ª–∞ {source_file_path} —É—Å–ø–µ—à–Ω–æ —Å–∫–æ–ø–∏—Ä–æ–≤–∞–Ω–æ –≤ —Ñ–∞–π–ª {target_file_path}.")


